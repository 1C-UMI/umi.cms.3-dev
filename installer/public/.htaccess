RewriteEngine On

RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]
##
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^admin /admin.php [NC,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^.*$ /index.php [NC,L]
