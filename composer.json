{
    "name": "umi/cms",
    "description": "UMI.CMS 3 Development Package",
    "type": "library",
    "license": "proprietary",
    "homepage": "http://umi-cms.ru/",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "umicms\\": "umicms/library",
            "umicms\\project\\" : "umicms/project",
            "demohunt\\" : "demohunt"
        }
    },
    "replace": {
        "umi/cms" : "self.version"
    },
    "require": {
        "php": ">=5.4.0",
        "umi/framework-dev": "*",
        "symfony/form": "2.4.2"
    },
    "repositories":[
        {
            "type": "git",
            "url": "git@gl.umisoft.ru:umisoft/umi-framework-dev.git"
        }
    ]
}
