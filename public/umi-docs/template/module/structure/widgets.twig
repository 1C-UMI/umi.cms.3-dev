<article class="col-sm-8">

    <h1>{{ page.getHeader() }}</h1>

    {% if (page.getTypeName() == 'static.widget') %}
        <p> Имя виджета: {{ page.displayName}} </p>

        {{ page.description | raw }}
        {{ page.contents | raw }}
        {{ page.parameters | raw }}
        {{ page.returnValue | raw }}

        {{ page.secondContents | raw }}
    {% else %}
        {{ page.contents | raw }}
        {{ widget('structure.menu.auto', {'depth': 10, 'branch' : page, 'template' : 'tree'})|raw }}
    {% endif %}

</article>

<aside class="col-sm-3 col-sm-offset-1">
    {{ widget('structure.menu.auto', {'branch' : 'd7dda227-cac7-474d-ab0d-d361d0bc16a3', 'template' : 'side'})|raw }}
</aside>