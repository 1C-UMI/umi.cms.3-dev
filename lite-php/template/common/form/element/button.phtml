<?php
/**
 * @var \umi\form\element\IFormButton $button
 */
$attributes = array_merge(
    $button->getAttributes(),
    [
        'type' => $button->getButtonType(),
        'name' => $button->getElementName(),
        'value' => $button->getValue()
    ]
);

echo '<button ' . buildAttributes($attributes) . ' />';